generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscriber {
  id        Int      @id @default(autoincrement())
  name      String
  surname   String
  email     String   @unique
  createdAt DateTime @default(now()) @map("created_at")

  @@map("subscribers")
}
